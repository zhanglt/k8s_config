apiVersion: v1
kind: Service
metadata:
  labels:
    app: grafana
    chart: grafana-4.2.2
    heritage: Tiller
    release: grafana
  name: grafana-nodeport
  namespace: kube-monitor
spec:
 
  ports:
  - name: service
    port: 80
    protocol: TCP
    targetPort: 3000
    nodePort: 39092
  selector:
    app: grafana
    release: grafana
  sessionAffinity: None
  type: NodePort
status:
  loadBalancer: {}
